{
  "name": "project-350-testing",
  "version": "1.0.0",
  "description": "API testing suite for Project-350 travel assistant",
  "main": "test-api.js",
  "scripts": {
    "test": "node test-api.js",
    "test:api": "node test-api.js",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../weather-350 && npm install",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "start:weather": "cd weather-350 && npm start",
    "docs:generate": "cd backend && node config/swagger.js",
    "docs:serve": "cd backend && npm run swagger"
  },
  "dependencies": {
    "react-router-dom": "^7.5.1",
    "axios": "^1.6.0",
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "travel",
    "api",
    "testing",
    "expense-tracker",
    "emergency-assistance",
    "nodejs",
    "express"
  ],
  "author": "Project-350 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "project-350"
  }
}
